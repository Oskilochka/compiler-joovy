// Test if-else statements
int x = -15
int y = 5

print("x =", x)
print("y =", y)

if (x > y) {
    print("x is greater than y")
} else {
    if(x < y) {
        print("x is not greater than y")
    }
    else {
        print("x equals y")
    }
}

// Nested if
if (x > 0) {
    if (y > 0) {
        print("Both positive")
    } else {
        print("x positive, y not")
    }
} else {
    print("x negative")
}
