int n = 5

// Nested loop
for (i in 1..3) {
    for (j in 1..3) {
        int product = i * j
        print(i, "*", j, "=", product)
    }
}

// Nested conditional statements
if (n > 0) {
    if (n < 10) {
        print("n in range [1, 9]")
    } else {
        print("n >= 10")
    }
} else {
    print("n <= 0")
}

// Numbers with an exponent
float sci1 = 2.5e2
float sci2 = 1.2e-3
float sci3 = .5
float sci4 = 23.

print("Exponential numbers:")
print("2.5e2 =", sci1)
print("1.2e-3 =", sci2)

